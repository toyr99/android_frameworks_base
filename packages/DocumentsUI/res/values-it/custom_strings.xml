<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Action bar title when the app is in standalone (file manager) mode [CHAR LIMIT=32] -->
    <string name="title_standalone">File</string>

    <!-- Toast shown after successfully deleting files -->
    <string name="toast_success_delete">File eliminato/i</string>

    <!-- Title of the Delete confirmation dialog -->
    <string name="dialog_delete_confirm_title">Eliminare file?</string>

    <!-- Menu item title that copies the selected documents [CHAR LIMIT=24] -->
    <string name="menu_copy">Copia</string>
    <!-- Menu item title that cuts the selected documents [CHAR LIMIT=24] -->
    <string name="menu_cut">Taglia</string>
    <!-- Menu item title that pastes the selected documents [CHAR LIMIT=24] -->
    <string name="menu_paste">Incolla</string>


    <!-- Text shown in a toast when the user copies files -->
    <plurals name="files_copied">
        <item quantity="one">Il file è stato copiato negli appunti</item>
        <item quantity="other">%d file sono stati copiati negli appunti</item>
    </plurals>

    <!-- Text shown in a toast when the user pastes files -->
    <plurals name="files_pasted">
        <item quantity="one">Il file è stato incollato. L\'operazione potrebbe finire in background.</item>
        <item quantity="other">%d file sono stati incollati. L\'operazione potrebbe finire in background.</item>
    </plurals>

    <!-- Text show in the progress dialog while files are being copied -->
    <string name="copy_in_progress">Copia...</string>

    <!-- Text show in the progress dialog while files are being deleted -->
    <string name="delete_in_progress">Elimina...</string>

    <plurals name="dialog_delete_confirm_message">
        <item quantity="one">Questo file sarà eliminato permanentemente!</item>
        <item quantity="other">Questi %d file saranno eliminati permanentemente!</item>
    </plurals>

    <string name="ok">OK</string>
    <string name="cancel">Annulla</string>
</resources>
